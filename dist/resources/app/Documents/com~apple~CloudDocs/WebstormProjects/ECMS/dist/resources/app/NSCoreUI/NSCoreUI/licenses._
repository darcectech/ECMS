<div id="licenses" material>
    <h2>License Generator</h2>

    <div scroller xl>
        <div class="input-field col s12">
            <select id="licenseList">
                <option value="" disabled selected>License Type</option>

            </select>
            <label>Select a License to generate</label>
        </div>
        <div class="row">
            <div class="input-field col s6">
                <input value="" id="first_name2" type="text">
                <label class="active" for="first_name2">First Name</label>
            </div>
            <div class="input-field col s6">
                <input id="email" type="email">
                <label for="email">Email</label>
            </div>
        </div>

        <div class="licenseView">
            <pre></pre>
        </div>

        <div class="row">
            <div class="col s4 m4 l4">
                <a href="#!" class="btn waves-effect waves-light cyan accent-4" onclick="pages.licenses.showLicenseSelected()">Generate</a>
            </div>
            <div class="col s4 m4 l4">
                <a href="#!" class="btn waves-effect waves-light cyan accent-4" onclick="pages.licenses.copyLicense()">Copy</a>
            </div>
            <div class="col s4 m4 l4">
                <a href="#!" class="btn waves-effect waves-light cyan accent-4" onclick="pages.licenses.saveLicense()">Save as File</a>
            </div>
        </div>
    </div>

</div>